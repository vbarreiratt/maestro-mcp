{
  "name": "maestro-mcp",
  "version": "1.0.0",
  "description": "Professional MCP server for musical AI with real-time MIDI control",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "NODE_ENV=production MCP_MODE=true node dist/server.js",
    "start:debug": "NODE_ENV=development node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "test": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:musical": "vitest run tests/musical",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src/**/*.ts",
    "install:claude": "node scripts/install-claude-desktop.js",
    "install:claude-cli": "claude mcp add maestro --env NODE_ENV=production --env MCP_MODE=true -- node $(pwd)/dist/server.js",
    "inspect": "npm run build && npx @modelcontextprotocol/inspector -- node dist/server.js",
    "inspect:manual": "npx @modelcontextprotocol/inspector"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "midi",
    "music",
    "ai",
    "tone",
    "tonal",
    "musical-ai"
  ],
  "author": "Maestro MCP",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@supabase/supabase-js": "^2.55.0",
    "@tonaljs/chord": "^6.1.1",
    "@tonaljs/core": "^5.0.2",
    "@tonaljs/key": "^4.11.2",
    "@tonaljs/note": "^4.12.1",
    "@tonaljs/scale": "^4.13.3",
    "@tonaljs/tonal": "^4.10.0",
    "dotenv": "^16.3.1",
    "jzz": "^1.7.9",
    "tone": "^15.0.0",
    "uuid": "^11.1.0",
    "zod": "^3.22.0",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.0",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ]
}
